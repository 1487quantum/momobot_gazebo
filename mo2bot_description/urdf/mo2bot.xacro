<?xml version='1.0'?>

<robot name="myrobot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:property name="cameraSize" value="0.05"/>
  <xacro:property name="cameraMass" value="0.1"/>

  <xacro:include filename="$(find mo2bot_description)/urdf/mo2bot.gazebo" />
  <xacro:include filename="$(find mo2bot_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find mo2bot_description)/urdf/macros.xacro" />

  <!--Moment of Inertia: https://en.wikipedia.org/wiki/List_of_moments_of_inertia, checkout 3D inertial tensor -->



  <link name='base_link'>
    <pose>0 0 0 0 0 0</pose>
</link>

<joint name="chassis_to_baseLink" type="fixed">
  <axis xyz="0 0 0"/>
  <parent link="base_link"/>
  <child link="chassis"/>
  <origin xyz="0 0 0" rpy="0 0 0" />
</joint>

  <link name='chassis'>
    <inertial>
      <mass value="50.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <!-- Inertia calculation based on m=50, w=h=0.52, d=0.84-->
      <inertia
        ixx="4.0667" ixy="0" ixz="0"
        iyy="4.0667" iyz="0"
        izz="2.2533"
        />
      </inertial>

      <collision name='collision'>
        <geometry>
          <box size=".52 .52 .84"/>
        </geometry>
      </collision>

      <!-- The chassis would be split into 2 parts: top & bottom-->
      <!--Top-->

      <visual name='chassis_visual_top'>
        <origin xyz="0 0 0.22" rpy=" 0 0 0"/>
        <geometry>
          <box size=".52 .52 .44"/>
        </geometry>
      </visual>
      <!--Bottom-->
      <visual name='chassis_visual_bottom'>
        <origin xyz="-0.10 0 -0.2" rpy=" 0 0 0"/>
        <geometry>
          <box size=".30 .52 .4"/> <!--x,y,z-->
        </geometry>
      </visual>


      <!--2 beams out-->
      <visual name='chassis_visual_bottom_left'>
        <origin xyz="0 -0.22 -0.32" rpy=" 0 0 0"/>
        <geometry>
          <box size=".52 .05 .05"/> <!--x,y,z-->
        </geometry>
      </visual>

      <visual name='chassis_visual_bottom_right'>
        <origin xyz="0 0.22 -0.32" rpy=" 0 0 0"/>
        <geometry>
          <box size=".52 .05 .05"/> <!--x,y,z-->
        </geometry>
      </visual>
    </link>

    <!--Caster Wheels-->
    <!--link name='caster_left'>
      <collision name='caster_collision_left'>
