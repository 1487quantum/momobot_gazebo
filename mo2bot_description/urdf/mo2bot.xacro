    <!-- Caster joint, right -->
    <!--joint name="cst_joint_right" type="continuous">
      <axis xyz="1 1 0"/>
      <parent link="chassis"/>
      <child link="caster_right"/>
      <origin xyz="0.22 0.24 -0.4" rpy="0 0 0"/>
    </joint-->

<!--Left trolley wheel-->
    <link name='trolleyW_wheel_front_left'>
      <collision name='collision_trolleyW_wheel_front_left'>
        <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
        <geometry>
          <cylinder radius="0.05" length="0.03"/>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>

      <visual name='visual_trolleyW_wheel_front_left'>
        <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
        <geometry>
          <cylinder radius="0.05" length="0.03"/>
        </geometry>
        <material name="grey"/>
      </visual>

      <!-- Inertia calculation based on m=0.8, h=0.03, r=0.05-->
      <inertial>
        <mass value="0.8"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <inertia
          ixx="560e-6" ixy="0" ixz="0"
          iyy="560e-6" iyz="0"
          izz="0.001"   />
      </inertial>
    </link>

    <joint name="joint_trolleyW_wheel_front_left" type="continuous">
      <axis xyz="0 1 0"/>
      <parent link="trolleyW_holder_front_left"/>
      <child link="trolleyW_wheel_front_left"/>
      <origin xyz="-0.04 0 -0.06" rpy="0 0 0" />
    </joint>

<!--Trolley wheel holder attachment-->
    <link name='trolleyW_holder_front_left'>
      <collision name='collision_trolleyW_holder_front_left'>
        <origin xyz="-0.03 0 -0.0075" rpy=" 0 0 0"/>
        <geometry>
          <box size=".1 .03 .015"/>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>

      <visual name='visual_trolleyW_holderHorizontal_front_left'>
        <origin xyz="-0.03 0 -0.0075" rpy=" 0 0 0"/>
        <geometry>
          <box size=".1 .03 .015"/>
        </geometry>
        <material name="grey"/>
      </visual>

      <visual name='visual_trolleyW_holderVertical_front_left'>
        <origin xyz="-0.04 0 -0.03" rpy=" 0 0 0"/>
        <geometry>
          <box size=".015 .03 .06"/>
        </geometry>
        <material name="grey"/>
      </visual>

      <!-- Inertia calculation based on m=0.05, w = 0.1, h=0.03, d=0.015-->
      <inertial>
        <mass value="1"/>
        <origin rpy="-0.02 0 0" xyz="0 0 0"/>
        <inertia
          ixx="4.6875e-6" ixy="0" ixz="0"
          iyy="42.6041e-6" iyz="0"
          izz="45.4167e-6"   />
      </inertial>
    </link>

    <joint name="joint_trolleyW_front_left" type="continuous">
      <axis xyz="0 0 1"/>
      <parent link="chassis"/>
      <child link="trolleyW_holder_front_left"/>
      <origin xyz="0.22 0.22 -0.345" rpy="0 0 0" />
    </joint>



    <!--Right trolley wheel-->
        <link name='trolleyW_wheel_front_right'>
          <collision name='collision_trolleyW_wheel_front_right'>
            <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
            <geometry>
              <cylinder radius="0.05" length="0.03"/>
            </geometry>
            <surface>
              <friction>
                <ode>
                  <mu>0</mu>
                  <mu2>0</mu2>
                  <slip1>1.0</slip1>
                  <slip2>1.0</slip2>
                </ode>
              </friction>
            </surface>
          </collision>

          <visual name='visual_trolleyW_wheel_front_right'>
            <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
            <geometry>
              <cylinder radius="0.05" length="0.03"/>
            </geometry>
            <material name="grey"/>
          </visual>

          <!-- Inertia calculation based on m=0.8, h=0.03, r=0.05-->
          <inertial>
            <mass value="0.8"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia
              ixx="560e-6" ixy="0" ixz="0"
              iyy="560e-6" iyz="0"
              izz="0.001"   />
          </inertial>
        </link>

        <joint name="joint_trolleyW_wheel_front_right" type="continuous">
          <axis xyz="0 1 0"/>
          <parent link="trolleyW_holder_front_right"/>
          <child link="trolleyW_wheel_front_right"/>
          <origin xyz="-0.04 0 -0.06" rpy="0 0 0" />
        </joint>

    <!--Trolley wheel holder attachment-->
        <link name='trolleyW_holder_front_right'>
          <collision name='collision_trolleyW_holder_front_right'>
            <origin xyz="-0.03 0 -0.0075" rpy=" 0 0 0"/>
            <geometry>
              <box size=".1 .03 .015"/>
            </geometry>
            <surface>
              <friction>
                <ode>
                  <mu>0</mu>
                  <mu2>0</mu2>
                  <slip1>1.0</slip1>
                  <slip2>1.0</slip2>
                </ode>
              </friction>
            </surface>
          </collision>

          <visual name='visual_trolleyW_holderHorizontal_front_right'>
            <origin xyz="-0.03 0 -0.0075" rpy=" 0 0 0"/>
            <geometry>
              <box size=".1 .03 .015"/>
            </geometry>
            <material name="grey"/>
          </visual>

          <visual name='visual_trolleyW_holderVertical_front_right'>
            <origin xyz="-0.04 0 -0.03" rpy=" 0 0 0"/>
            <geometry>
              <box size=".015 .03 .06"/>
            </geometry>
            <material name="grey"/>
          </visual>

          <!-- Inertia calculation based on m=0.05, w = 0.1, h=0.03, d=0.015-->
          <inertial>
            <mass value="1"/>
            <origin rpy="-0.02 0 0" xyz="0 0 0"/>
            <inertia
              ixx="4.6875e-6" ixy="0" ixz="0"
              iyy="42.6041e-6" iyz="0"
              izz="45.4167e-6"   />
          </inertial>
        </link>

        <joint name="joint_trolleyW_front_right" type="continuous">
          <axis xyz="0 0 1"/>
          <parent link="chassis"/>
          <child link="trolleyW_holder_front_right"/>
          <origin xyz="0.22 -0.22 -0.345" rpy="0 0 0" />
        </joint>




    <!--Main Wheels-->
    <!-- Inertia calculation based on m=2, h=0.05, r=0.1-->
    <link name="left_wheel">
      <!--origin xyz="0.1 0.13 0.1" rpy="0 1.5707 1.5707"/-->
      <collision name="collision">
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
        <geometry>
          <cylinder radius="0.1" length="0.05"/>
        </geometry>
      </collision>

      <visual name="left_wheel_visual">
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
        <geometry>
          <cylinder radius="0.1" length="0.05"/>
        </geometry>
        <material name="black"/>
      </visual>

      <inertial>
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
        <mass value="5"/>
        <inertia
          ixx="5.4167e-3" ixy="0.0" ixz="0.0"
          iyy="5.4167e-3" iyz="0.0"
          izz="0.01"/>
        </inertial>
      </link>

      <!--Left wheel joint-->
      <joint type="continuous" name="left_wheel_joint">
        <origin xyz="-0.124 -0.19 -.35" rpy="0 0 0"/>
        <!--origin xyz="0.1 0.13 0" rpy="0 1.5707 1.5707"/-->
        <child link="left_wheel"/>
        <parent link="chassis"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
      </joint>

      <link name="right_wheel">
        <!--origin xyz="0.1 -0.13 0.1" rpy="0 1.5707 1.5707"/-->
        <collision name="collision">
          <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
          <geometry>
            <cylinder radius="0.1" length="0.05"/>
          </geometry>
        </collision>
        <visual name="right_wheel_visual">
          <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
          <geometry>
            <cylinder radius="0.1" length="0.05"/>
          </geometry>
          <material name="black"/>
        </visual>
        <inertial>
          <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
          <mass value="5"/>
          <inertia
            ixx="5.4167e-3" ixy="0.0" ixz="0.0"
            iyy="5.4167e-3" iyz="0.0"
            izz="0.01"/>
          </inertial>
        </link>

        <!--Right Wheel Joint-->
        <joint type="continuous" name="right_wheel_joint">
          <origin xyz="-0.124 0.19 -.35" rpy="0 0 0"/>
          <!--origin xyz="0.1 -0.13 0" rpy="0 1.5707 1.5707"/-->
          <child link="right_wheel"/>
          <parent link="chassis"/>
          <axis xyz="0 1 0" rpy="0 0 0"/>
          <limit effort="10000" velocity="1000"/>
          <joint_properties damping="1.0" friction="1.0"/>
        </joint>

        <!--Lidar-->
        <joint name="sick_joint" type="fixed">
          <axis xyz="0 1 0" />
          <origin xyz=".3 0 -.22" rpy="3.1416 0 0"/>
          <parent link="chassis"/>
          <child link="sick_l"/>
        </joint>

        <!-- SICK Laser -->

        <link name="sick_l">
          <collision>
            <origin xyz="0 0 -0.05" rpy="0 0 0"/>
            <geometry>
              <box size="0.105 0.102 0.28"/>
            </geometry>
          </collision>
          <!--Top-->
          <visual name='sick_visual_top'>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
              <cylinder radius="0.0525" length="0.03"/>
            </geometry>
            <material name="grey"/>
          </visual>
          <!--Body-->
          <visual name='sick_visual_body'>
            <origin xyz="0 0 -0.067" rpy="0 0 0"/>
            <geometry>
              <box size="0.105 0.102 0.1"/>
            </geometry>
            <material name="white"/>
          </visual>

          <inertial>
            <mass value="1.1" />
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
          </inertial>
        </link>

        <!--Camera-->

        <link name="camera">
          <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
              <box size="${cameraSize} ${cameraSize} ${cameraSize}"/>
            </geometry>
          </collision>

          <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
              <box size="${cameraSize} ${cameraSize} ${cameraSize}"/>
            </geometry>
            <material name="green"/>
          </visual>

          <inertial>
            <mass value="${cameraMass}" />
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <box_inertia m="${cameraMass}" x="${cameraSize}" y="${cameraSize}" z="${cameraSize}" />
            <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
          </inertial>
        </link>

        <joint name="camera_joint" type="fixed">
          <axis xyz="0 1 0" />
          <origin xyz=".2 0 0" rpy="0 0 0"/>
          <parent link="chassis"/>
          <child link="camera"/>
        </joint>


        <!-- Depth Camera, Back -->
        <!-- ZED Cam https://www.stereolabs.com/zed/ -->
        <link name="dcam_back">
          <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
              <box size=".03 .175 .033"/>
            </geometry>
          </collision>

          <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
              <box size=".03 .175 .033"/>
            </geometry>
            <material name="grey"/>
          </visual>

          <inertial>
            <mass value="${cameraMass}" />
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <box_inertia m="0.159" x=".03" y=".175" z=".033" />
            <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
          </inertial>
        </link>

        <joint name="joint_dcam_back" type="fixed">
          <axis xyz="0 1 0" />
          <origin xyz="-.27 0 0.2" rpy="0 0 3.1416"/>
          <parent link="chassis"/>
          <child link="dcam_back"/>
        </joint>

        <!--Camera to Depth link -->

        <joint name="joint_dcam_back_depth" type="fixed">
    <origin xyz="0 0 0" rpy="${-3.1416/2} 0 ${-3.1416/2} "/>
    <parent link="dcam_back"/>
    <child link="dcam_back_depth"/>
  </joint>

  <link name="dcam_back_depth"></link>

