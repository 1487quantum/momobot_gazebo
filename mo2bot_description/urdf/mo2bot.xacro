      <origin xyz="0.22 -0.24 -0.4" rpy="0 0 0" />
    </joint-->

    <!--Caster wheel, Right-->
    <!--link name='caster_right'>
      <collision name='caster_collision_right'>
        <origin xyz="0 0 0" rpy=" 0 0 0"/>
        <geometry>
          <sphere radius="0.05"/>
        </geometry>
        <surface>
          <friction>
            <ode>
